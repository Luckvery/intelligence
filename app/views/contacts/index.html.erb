<div class="well span10">
    <%= render 'header'%>

    <table class="table table-striped table-condensed">
        <% @contacts.each do |contact| %>
            <tr class="list">
                <td>
                    <!--BLANK BUFFER SPACE-->
                </td>
                <td>
                    <h4>  <%= link_to contact.contactFullName, contact %>   </h4>
                </td>
                <td>
                    <h2>  <%= link_to "<i class=\"icon-list icon-white\"></i>  Edit".html_safe, edit_contact_path(contact), :class => "btn btn-large btn-info" %></h2>
                </td>
            </tr>
        <% end %>
    </table>

    <br/>

    <%= link_to "<i class=\"icon-user icon-white\"></i>   New Contact".html_safe, new_contact_path, :class => "btn btn-large btn-primary btn-block" %>

</div>