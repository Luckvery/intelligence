<div class="span12">
    <%= render :partial => "header",
               :locals => { :header_title => "Contact Profiles" } %>
</div>
<div class="span12">


        <table id="contactTable" class="table table-striped table-bordered">
            <thead>
            <tr>
                <th  >Name</th>
                <th>Last Edited</th>
            </tr>
            </thead>
            <tbody>

        <% @contacts.each do |contact| %>
            <tr>
                <td>
                    <h4>  <%= link_to contact.contactFullName, contact %>   </h4>
                </td>
                <td>
                      <%= link_to "<i class=\"icon-list icon-white\"></i>  Edit".html_safe, edit_contact_path(contact), :class => "btn btn-mini btn-info" %>
                </td>
            </tr>
        <% end %>
    </table>

    <br/>

    <%= link_to "<i class=\"icon-user icon-white\"></i>   New Contact".html_safe, new_contact_path, :class => "btn btn-large btn-primary btn-block" %>

</div>
